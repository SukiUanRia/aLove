
import turtle

def curvemove():
  for i in range(200):
    turtle.right(1)
    turtle.forward(1)

turtle.color('pink')
turtle.begin_fill()
turtle.left(140)
turtle.forward(111.65)
curvemove()
turtle.left(120)
curvemove()
turtle.forward(111.65)
turtle.end_fill()

turtle.penup()
turtle.goto(-10, -50)
turtle.pendown()
turtle.write('',  font = ('Lucida Console', 15, 'bold'))
turtle.hideturtle()


import os
print("Sth wrong in PTG please exit in console")#翻译大法好#
os.system('pause')
